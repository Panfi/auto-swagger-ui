{
    "name": "kevupton/auto-swagger-ui",
    "description": "Want the swagger ui without having to set it up. Just install this package.",
    "keywords": ["swagger-ui", "php"],
    "type": "Extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Upton",
            "email": "uptonkg@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "kevupton/laravel-swagger": "^1.3"
    },
    "require-dev": {
        "laravel/framework": "5.5.*",
        "laravel/lumen-framework": "5.5.*",
        "barryvdh/laravel-ide-helper": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Kevupton\\AutoSwaggerUI\\": "src/Kevupton/AutoSwaggerUI"
        },
        "files": [
            "src/Kevupton/AutoSwaggerUI/Helpers/helpers.php"
        ]
    },
    "minimum-stability": "stable"
}
